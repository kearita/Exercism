# | Register | Usage  | Type    | Description            |
# | -------- | ------ | ------- | ---------------------- |
# | `$v0`    | output | address | null-terminated string |

.globl hello

.data

msg: .asciiz "Hello, World!"

.text

hello:
        la $v0, msg   # put address of msg into v0 (special function reg)
        jr $ra        # jump to ra (return address reg)
